#include <iostream>
using namespace std;
int search(int arr[],int n,int num) {
    for(int i=0;i<n;i++){
        if(arr[i]==num) {
            cout<<arr[i];
            return i;
        }
    }
    return -1;

}
int main() {
 int arr[]={1,0,9,5,0,3,9};
   int num =3;
   int n =sizeof(arr)/sizeof(arr[0]);
       int val=search(arr,n,num);
       cout<<val<<endl;
    return 0; 
}
